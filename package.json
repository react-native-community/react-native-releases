{
  "name": "react-native-releases",
  "description": "Changelog for the React Native project",
  "license": "MIT",
  "scripts": {
    "format": "prettier *.md scripts{,/**}/*.js --write",
    "format:check": "prettier *.md scripts{,/**}/*.js --list-different",
    "generate": "./scripts/changelog-generator.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format:check"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-native-community/releases.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/react-native-community/releases/issues"
  },
  "homepage": "https://github.com/react-native-community/releases#readme",
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "husky": "1.3.1",
    "jest": "^25.1.0",
    "prettier": "1.17.0"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "deepmerge": "^4.2.2",
    "fast-levenshtein": "^2.0.6",
    "p-limit": "^2.2.2",
    "yargs": "13.2.2"
  }
}
